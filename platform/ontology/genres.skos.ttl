@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix muziq: <http://muziq.local/ontology#> .
@prefix audioset: <http://research.google.com/audioset/ontology/> .
@prefix lcgft: <http://id.loc.gov/authorities/genreForms/> .

<http://muziq.local/genre-scheme/lcgft+audioset>
  a skos:ConceptScheme ;
  skos:prefLabel "MuzIQ Genre Scheme (LCGFT + AudioSet seed)"@en ;
  skos:note "Initial seed ontology for MuzIQ AI classification. Derived from Library of Congress Genre/Form Terms (LCGFT) and Google's AudioSet ontology."@en .

# Classical
<lcgft:gf2014026680>
  a skos:Concept ;
  skos:prefLabel "Classical music"@en ;
  skos:inScheme <http://muziq.local/genre-scheme/lcgft+audioset> ;
  skos:exactMatch audioset:Classical_music .

